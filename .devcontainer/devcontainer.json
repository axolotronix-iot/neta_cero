{
  "name": "Neta Cero IoT Dev Environment",
  "dockerComposeFile": "../docker-compose.yml",
  "service": "dev",
  "workspaceFolder": "/workspace",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-azuretools.vscode-docker",
        "ms-python.python"
      ],
      "settings": {
        "python.pythonPath": "/workspace/venv/bin/python"
      }
    }
  }
}